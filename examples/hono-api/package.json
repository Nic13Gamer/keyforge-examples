{
  "name": "hono-api",
  "version": "1.0.0",
  "description": "A Hono API using the Keyforge Node.js SDK",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format cjs,esm --dts",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@types/node": "^20.14.1",
    "tsup": "^8.1.0",
    "tsx": "^4.11.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.2",
    "dotenv": "^16.4.5",
    "hono": "^4.4.3",
    "keyforge-js": "^0.2.1"
  }
}
